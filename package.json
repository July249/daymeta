{
  "name": "@nostos/calendar",
  "version": "0.1.0",
  "description": "Korean date intelligence engine with lunar calendar, holidays, and special days support",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "node build.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "tsc --noEmit",
    "generate-data": "tsx scripts/generate-offline-data.ts",
    "prepublishOnly": "npm run lint && npm run test && npm run build"
  },
  "keywords": [
    "korean",
    "calendar",
    "lunar",
    "holiday",
    "korea",
    "kst",
    "24절기",
    "음력",
    "공휴일"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "^18.19.68",
    "esbuild": "^0.24.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "dependencies": {}
}
